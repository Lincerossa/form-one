{
  "name": "form-one",
  "description": "react-hook-form abstraction for controlled antd inputs",
  "version": "1.0.0",
  "homepage": "https://form-one.netlify.app",
  "main": "dist",
  "author": {
    "name": "Luatti Marcello",
    "email": "marcelloluatti@gmail.com"
  },
  "scripts": {
    "start": "npx styleguidist server",
    "build": "npx styleguidist build",
    "test": "react-scripts test",
    "transpile": "babel src/Form -d dist --presets minify --copy-files",
    "release": "semantic-release --no-ci"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/npm"
    ],
    "branches": [
      "master"
    ]
  },
  "dependencies": {
    "@ant-design/compatible": "^1.0.8",
    "@hookform/resolvers": "^1.3.4",
    "antd": "^4.12.3",
    "get-value": "^3.0.1",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-hook-form": "^6.15.1",
    "styled-components": "^5.2.1",
    "yup": "^0.32.8"
  },
  "peerDependencies": {
    "react": ">=17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "ast-types": "^0.14.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.2",
    "react-styleguidist": "^11.1.5",
    "semantic-release": "^17.3.8"
  }
}
